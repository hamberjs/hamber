{
  "name": "hamber",
  "version": "3.24.0",
  "description": "Cybernetically enhanced web apps",
  "module": "index.mjs",
  "main": "index",
  "files": [
    "types",
    "compiler.*",
    "register.js",
    "index.*",
    "internal",
    "store",
    "animate",
    "transition",
    "easing",
    "motion",
    "hamber",
    "README.md"
  ],
  "engines": {
    "node": ">= 8"
  },
  "types": "types/runtime/index.d.ts",
  "scripts": {
    "build": "rollup -c && npm run tsd",
    "prepare": "npm run build",
    "dev": "rollup -cw",
    "prepublishOnly": "npm run lint",
    "tsd": "tsc -p src/compiler --emitDeclarationOnly && tsc -p src/runtime --emitDeclarationOnly",
    "lint": "eslint \"src/**/*.{ts,js}\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hamberjs/hamber.git"
  },
  "keywords": [
    "UI",
    "framework",
    "templates",
    "templating"
  ],
  "author": "NKDuy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hamberjs/hamber/issues"
  },
  "homepage": "https://github.com/hamberjs/hamber#README",
  "devDependencies": {
    "@hamberjs/eslint-config": "github:hamberjs/eslint-config#v0.0.1",
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-json": "^4.0.1",
    "@rollup/plugin-node-resolve": "^6.0.0",
    "@rollup/plugin-replace": "^2.3.0",
    "@rollup/plugin-sucrase": "^3.0.0",
    "@rollup/plugin-typescript": "^2.0.1",
    "@rollup/plugin-virtual": "^2.0.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^8.10.53",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "acorn": "^7.3.1",
    "agadoo": "^1.1.0",
    "c8": "^5.0.1",
    "code-red": "^0.1.3",
    "codecov": "^3.5.0",
    "css-tree": "1.0.0-alpha22",
    "eslint": "^7.1.0",
    "eslint-plugin-hamber3": "^2.7.3",
    "eslint-plugin-import": "^2.20.2",
    "estree-walker": "^1.0.0",
    "is-reference": "^1.1.4",
    "jsdom": "^15.1.1",
    "kleur": "^3.0.3",
    "locate-character": "^2.0.5",
    "magic-string": "^0.25.3",
    "mocha": "^6.2.0",
    "periscopic": "^2.0.1",
    "puppeteer": "^2.1.1",
    "rollup": "^1.27.14",
    "source-map": "^0.7.3",
    "source-map-support": "^0.5.13",
    "tiny-glob": "^0.2.6",
    "tslib": "^1.10.0",
    "typescript": "^3.5.3"
  },
  "nyc": {
    "include": [
      "compiler/hamber.js",
      "shared.js"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
